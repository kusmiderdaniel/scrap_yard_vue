{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'Dashboard',\n  data() {\n    return {\n      receipts: [],\n      team: {},\n      gross_total_value: 0,\n      total_quantity: 0\n    };\n  },\n  mounted() {\n    this.getReceipts(), this.getTeam();\n  },\n  methods: {\n    getReceipts() {\n      axios.get('/api/v1/receipts/').then(response => {\n        // Filter receipts to only include today's date\n        const today = new Date().toISOString().split('T')[0];\n        this.receipts = response.data.filter(receipt => receipt.date === today);\n        let gross_total_value = 0;\n        let total_quantity = 0;\n        for (let i = 0; i < this.receipts.length; i++) {\n          gross_total_value += Number(this.receipts[i].gross_amount);\n          total_quantity += Number(this.receipts[i].total_quantity);\n        }\n        this.gross_total_value = gross_total_value.toFixed(2);\n        this.total_quantity = total_quantity.toFixed(2);\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    },\n    getTeam() {\n      axios.get('/api/v1/teams/').then(response => {\n        this.team = response.data[0];\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","receipts","team","gross_total_value","total_quantity","mounted","getReceipts","getTeam","methods","get","then","response","today","Date","toISOString","split","filter","receipt","date","i","length","Number","gross_amount","toFixed","catch","error","console","log","JSON","stringify"],"sources":["/Users/danielkusmider/Library/CloudStorage/OneDrive-Personal/Docs/Projects/ScrapYard/scrap_yard_vue/src/views/dashboard/Dashboard.vue"],"sourcesContent":["<template>\n    <div class=\"page-dashboard\">\n        <div class=\"columns is-multiline\">\n            <div class=\"column is-12\">\n                <h1 class=\"title\">Dashboard</h1>\n            </div>\n\n            <div class=\"column is-8\" v-if=\"receipts\">\n                <div class=\"box\">\n                    <h1 class=\"title is-4\">Dzisiaj</h1>\n                    <div class=\"columns\">\n\n                        <div class=\"column is-4 level-item has-text-centered\">\n                            <div>\n                            <p class=\"heading\">Wypłacono</p>\n                            <p class=\"title\">{{ gross_total_value }} zł</p>\n                            </div>\n                        </div>\n\n                        <div class=\"column is-4 level-item has-text-centered\">\n                            <div>\n                            <p class=\"heading\">Odebrano</p>\n                            <p class=\"title\">{{ total_quantity }} kg</p>\n                            </div>\n                        </div>\n\n                        <div class=\"column is-4 level-item has-text-centered\">\n                            <div>\n                            <p class=\"heading\">Klienci</p>\n                            <p class=\"title\">{{ this.receipts.length }}</p>\n                            </div>\n                        </div>\n                        \n                    </div>\n                </div>\n\n                <div class=\"box\">\n                    <h2 class=\"title is-5\">Dzisiejsze kwity</h2>\n\n                    <table class=\"table is-fullwidth\">\n                        <thead>\n                            <tr>\n                                <th>Numer</th>\n                                <th>Klient</th>\n                                <th class=\"has-text-centered\">Data</th>\n                                <th style=\"width: 120px;\">Wartość</th>\n                                <th></th>\n                            </tr>\n                        </thead>\n\n                        <tbody>\n                            <tr\n                                v-for=\"receipt in receipts\"\n                                v-bind:key=\"receipt.id\"\n                            >\n                                <td>{{ receipt.receipt_number }}</td>\n                                <td>{{ receipt.client_name }}</td>\n                                <td class=\"has-text-centered\">{{ receipt.date }}</td>\n                                <td class=\"has-text-right\">{{ receipt.gross_amount }} zł</td>\n                                <td class=\"has-text-right\">\n                                    <router-link :to=\"{ name: 'Receipt', params: { id: receipt.id } }\">Szczegóły</router-link>\n                                </td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n\n            <div class=\"column is-4\">\n                <div class=\"box\">\n                    <h2 class=\"title is-5\">Twoja firma</h2>\n                    <router-link to=\"/dashboard/my-account/edit-team\" class=\"button is-light\">Edytuj dane firmy</router-link>\n                    <hr>\n                    <p><strong>{{ team.name }}</strong></p>\n                    <p><strong>{{ team.owner_name }}</strong></p>\n                    <p><strong>NIP:</strong> {{ team.org_number }}</p>\n                    <p><strong>REGON:</strong> {{ team.org_number2 }}</p>\n                    <p><strong>Nr telefonu:</strong> {{ team.phone_number }}</p>\n                    <p><strong>Email:</strong> {{ team.email }}</p>\n                    <p><strong>Address:</strong></p>\n                    <p>{{ team.address1 }}</p>\n                    <p>{{ team.zipcode }} {{ team.place }}</p>\n\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    name: 'Dashboard',\n    data() {\n        return {\n            receipts: [],\n            team: {},\n            gross_total_value: 0,\n            total_quantity: 0\n        }\n    },\n    mounted() {\n        this.getReceipts(),\n        this.getTeam()\n    },\n    methods: {\n        getReceipts() {\n            axios\n                .get('/api/v1/receipts/')\n                .then(response => {\n                    // Filter receipts to only include today's date\n                    const today = new Date().toISOString().split('T')[0]\n                    this.receipts = response.data.filter(receipt => receipt.date === today)\n\n                    let gross_total_value = 0\n                    let total_quantity = 0\n                    for (let i = 0; i < this.receipts.length; i++) {\n                        gross_total_value += Number(this.receipts[i].gross_amount)\n                        total_quantity += Number(this.receipts[i].total_quantity)\n                    }\n                    this.gross_total_value = gross_total_value.toFixed(2)\n                    this.total_quantity = total_quantity.toFixed(2)\n                })\n                .catch(error => {\n                    console.log(JSON.stringify(error))\n                })\n        },\n        getTeam() {\n            axios\n                .get('/api/v1/teams/')\n                .then(response => {\n                    this.team = response.data[0]\n                })\n                .catch(error => {\n                    console.log(JSON.stringify(error))\n                })\n        },\n    }\n}\n</script>"],"mappings":"AA0FA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,CAAC,CAAC;MACRC,iBAAiB,EAAE,CAAC;MACpBC,cAAc,EAAE;IACpB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,WAAW,CAAC,CAAC,EAClB,IAAI,CAACC,OAAO,CAAC;EACjB,CAAC;EACDC,OAAO,EAAE;IACLF,WAAWA,CAAA,EAAG;MACVR,KAAI,CACCW,GAAG,CAAC,mBAAmB,EACvBC,IAAI,CAACC,QAAO,IAAK;QACd;QACA,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC;QACnD,IAAI,CAACd,QAAO,GAAIU,QAAQ,CAACX,IAAI,CAACgB,MAAM,CAACC,OAAM,IAAKA,OAAO,C