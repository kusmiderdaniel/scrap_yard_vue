{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ItemForm',\n  props: {\n    initialItem: Object\n  },\n  data() {\n    return {\n      item: this.initialItem,\n      items: []\n    };\n  },\n  mounted() {\n    this.getItems();\n  },\n  methods: {\n    getItems() {\n      axios.get('/api/v1/items/').then(response => {\n        for (let i = 0; i < response.data.length; i++) {\n          this.items.push(response.data[i]);\n        }\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    },\n    itemChange() {\n      // Find the selected item from the items array\n      const selectedItemId = this.item.name.id;\n      const selectedItem = this.items.find(item => item.id === selectedItemId);\n\n      // Update the buy_price field with the selected item's buy_price\n      if (selectedItem) {\n        this.item.buy_price = selectedItem.buy_price;\n      }\n    }\n  },\n  computed: {\n    gross_amount() {\n      const buy_price = this.item.buy_price;\n      const quantity = this.item.quantity;\n      this.item.gross_amount = buy_price * quantity;\n      this.$emit('updatePrice', this.item);\n      return this.item.gross_amount;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","initialItem","Object","data","item","items","mounted","getItems","methods","get","then","response","i","length","push","catch","error","console","log","JSON","stringify","itemChange","selectedItemId","id","selectedItem","find","buy_price","computed","gross_amount","quantity","$emit"],"sources":["/Users/danielkusmider/Library/CloudStorage/OneDrive-Personal/Docs/Projects/ScrapYard/scrap_yard_vue/src/components/ItemForm.vue"],"sourcesContent":["<template>\n    <div class=\"columns\">\n\n        <div class=\"column is-3\">\n            <label>Produkt</label>\n\n            <div class=\"control\">\n                <div id=\"item-select\" class=\"select\">\n                    <select name=\"name\" v-model=\"item.name\" @change=\"itemChange($event)\">\n                        <option value=\"\">- wybierz produkt -</option>\n                        <option \n                            v-for=\"item in items\"\n                            v-bind:key=\"item.id\"\n                            v-bind:value=\"item\"\n                        >\n                            {{ item.name }}\n                        </option>\n                    </select>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"column is-2\">\n            <div class=\"field\">\n                <label>Cena skupu</label>\n                <div class=\"control\">\n                    <input type=\"number\" name=\"buy_price\" step=\"0.01\" class=\"input has-text-right\" v-model=\"item.buy_price\">\n                </div>\n            </div>\n        </div>\n\n        <div class=\"column is-2\">\n            <div class=\"field\">\n                <label>Ilość [kg]</label>\n                <div class=\"control\">\n                    <input type=\"number\" name=\"quantity\" step=\"0.01\" class=\"input has-text-right\" v-model=\"item.quantity\">\n                </div>\n            </div>\n        </div>\n\n        <div class=\"column is-2\">\n            <div class=\"field\">\n                <label>Wartość</label>\n                <div class=\"control\">\n                    <input type=\"text\" class=\"input has-text-right\" v-bind:value=\"gross_amount\" disabled>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    name: 'ItemForm',\n    props: {\n        initialItem: Object\n    },\n    data() {\n        return {\n            item: this.initialItem,\n            items: []\n        }\n    },\n    mounted() {\n        this.getItems()\n    },\n    methods: {\n        getItems() {\n            axios\n                .get('/api/v1/items/')\n                .then(response => {\n                    for (let i = 0; i < response.data.length; i++) {\n                        this.items.push(response.data[i])\n                    }\n                })\n                .catch(error => {\n                    console.log(JSON.stringify(error))\n                })\n        },\n        itemChange() {\n            // Find the selected item from the items array\n            const selectedItemId = this.item.name.id;\n            const selectedItem = this.items.find((item) => item.id === selectedItemId);\n\n            // Update the buy_price field with the selected item's buy_price\n            if (selectedItem) {\n                this.item.buy_price = selectedItem.buy_price;\n            }\n        },\n    },\n    computed: {\n        gross_amount() {\n            const buy_price = this.item.buy_price\n            const quantity = this.item.quantity\n\n            this.item.gross_amount = buy_price * quantity\n\n            this.$emit('updatePrice', this.item)\n\n            return this.item.gross_amount\n        }\n    }\n}\n</script>"],"mappings":";AAoDA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,WAAW,EAAEC;EACjB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,IAAI,CAACH,WAAW;MACtBI,KAAK,EAAE;IACX;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,QAAQ,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACLD,QAAQA,CAAA,EAAG;MACPT,KAAI,CACCW,GAAG,CAAC,gBAAgB,EACpBC,IAAI,CAACC,QAAO,IAAK;QACd,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAID,QAAQ,CAACR,IAAI,CAACU,MAAM,EAAED,CAAC,EAAE,EAAE;UAC3C,IAAI,CAACP,KAAK,CAACS,IAAI,CAACH,QAAQ,CAACR,IAAI,CAACS,CAAC,CAAC;QACpC;MACJ,CAAC,EACAG,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACC,IAAI,CAACC,SAAS,CAACJ,KAAK,CAAC;MACrC,CAAC;IACT,CAAC;IACDK,UAAUA,CAAA,EAAG;MACT;MACA,MAAMC,cAAa,GAAI,IAAI,CAAClB,IAAI,CAACL,IAAI,CAACwB,EAAE;MACxC,MAAMC,YAAW,GAAI,IAAI,CAACnB,KAAK,CAACoB,IAAI,CAAErB,IAAI,IAAKA,IAAI,CAACmB,EAAC,KAAMD,cAAc,CAAC;;MAE1E;MACA,IAAIE,YAAY,EAAE;QACd,IAAI,CAACpB,IAAI,CAACsB,SAAQ