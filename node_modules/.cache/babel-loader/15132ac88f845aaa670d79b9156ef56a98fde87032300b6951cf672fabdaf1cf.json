{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { toast } from 'bulma-toast';\nimport ItemForm from '@/components/ItemForm.vue';\nexport default {\n  name: 'AddReceipt',\n  components: {\n    ItemForm\n  },\n  data() {\n    return {\n      receipt: {\n        receipt_number: '',\n        date: new Date().toISOString().substr(0, 10),\n        client: '',\n        receipt_items: [{\n          item_num: 0,\n          name: '',\n          buy_price: '',\n          quantity: 1\n        }],\n        gross_amount: 0\n      },\n      clients: [],\n      team: {}\n    };\n  },\n  async mounted() {\n    await this.getClients(), await this.getTeam(), this.calculateReceiptNumber();\n  },\n  methods: {\n    getClients() {\n      axios.get('/api/v1/clients/').then(response => {\n        this.clients = response.data;\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    },\n    getTeam() {\n      axios.get('/api/v1/teams/').then(response => {\n        this.team = response.data[0];\n\n        // const formattedDate = this.formatDate(this.receipt.date)\n\n        // this.receipt.receipt_number = `${this.team.next_receipt_number}/${formattedDate}`\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    },\n    formatDate(date) {\n      const options = {\n        day: '2-digit',\n        month: '2-digit',\n        year: 'numeric'\n      };\n      return new Date(date).toLocaleDateString(undefined, options);\n    },\n    async calculateReceiptNumber() {\n      try {\n        const response = await axios.get(`/api/v1/receipts/?date=${this.receipt.date}`);\n        const existingReceipts = response.data;\n        console.log(existingReceipts);\n        const lastReceipt = existingReceipts[existingReceipts.length - 1];\n        if (lastReceipt) {\n          const lastReceiptNumberParts = lastReceipt.receipt_number.split('/');\n          const nextReceiptNumberPart1 = pars