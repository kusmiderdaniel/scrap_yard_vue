{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { toast } from 'bulma-toast';\nimport ItemForm from '@/components/ItemForm.vue';\nexport default {\n  name: 'AddReceipt',\n  components: {\n    ItemForm\n  },\n  data() {\n    return {\n      receipt: {\n        receipt_number: '',\n        client: '',\n        items: [{\n          item_num: 0,\n          name: '',\n          buy_price: '',\n          quantity: 1\n        }],\n        gross_amount: 0\n      },\n      clients: []\n    };\n  },\n  async mounted() {\n    await this.getClients();\n  },\n  methods: {\n    getClients() {\n      axios.get('/api/v1/clients/').then(response => {\n        this.clients = response.data;\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    },\n    addItem() {\n      this.receipt.items.push({\n        item_num: this.receipt.items.length,\n        category: '',\n        name: '',\n        buy_price: '',\n        quantity: 1\n      });\n    },\n    updateTotals(changedItem) {\n      let gross_amount = 0;\n      let item = this.receipt.items.filter(i => i.item_num === changedItem.item_num);\n      item = changedItem;\n      if (item) {\n        item.name = changedItem.name;\n      }\n      for (let i = 0; i < this.receipt.items.length; i++) {\n        gross_amount += this.receipt.items[i].gross_amount;\n      }\n      this.receipt.gross_amount = gross_amount;\n    },\n    submitForm() {\n      this.receipt.client_name = this.receipt.client.name;\n      this.receipt.client_email = this.receipt.client.email;\n      this.receipt.client_doc_number = this.receipt.client.doc_number;\n      this.receipt.client_address1 = this.receipt.client.address1;\n      this.receipt.client_address2 = this.receipt.client.address2;\n      this.receipt.client_zipcode = this.receipt.client.zipcode;\n      this.receipt.client_place = this.receipt.client.place;\n      this.receipt.client = this.receipt.client.id;\n      axios.post('/api/v1/receipts/', this.receipt).then(response => {\n        toast({\n          message: 'The receipt was added',\n          type: 'is-success',\n          dismissible: true,\n          pauseOnHover: true,\n          duration: 2000,\n          position: 'bottom-right'\n        });\n        this.$router.push('/dashboard/receipts');\n      }).catch(error => {\n        console.log(JSON.stringify(error));\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","toast","ItemForm","name","components","data","receipt","receipt_number","client","items","item_num","buy_price","quantity","gross_amount","clients","mounted","getClients","methods","get","then","response","catch","error","console","log","JSON","stringify","addItem","push","length","category","updateTotals","changedItem","item","filter","i","submitForm","client_name","client_email","email","client_doc_number","doc_number","client_address1","address1","client_address2","address2","client_zipcode","zipcode","client_place","place","id","post","message","type","dismissible","pauseOnHover","duration","position","$router"],"sources":["/Users/danielkusmider/Library/CloudStorage/OneDrive-Personal/Docs/Projects/ScrapYard/scrap_yard_vue/src/views/dashboard/AddReceipt.vue"],"sourcesContent":["<template>\n    <div class=\"page-add-receipt\">\n        <nav class=\"breadcrumb\" aria-label=\"breadcrumbs\">\n            <ul>\n                <li><router-link to=\"/dashboard\">Dashboard</router-link></li>\n                <li><router-link to=\"/dashboard/receipts\">Kwity</router-link></li>\n                <li class=\"is-active\"><router-link to=\"/dashboard/receipts/add-receipt\" aria-current=\"true\">Nowy kwit</router-link></li>\n            </ul>\n        </nav>\n\n        <div class=\"columns is-multiline\">\n            <div class=\"column is-12\">\n                <h1 class=\"title\">Nowy kwit</h1>\n            </div>\n\n            <div class=\"column is-3\">\n                <div class=\"field\">\n                    <label>Numer kwitu</label>\n\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"receipt.receipt_number\">\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"column is-12\">\n                <h2 class=\"is-size-5 mb-4\">Kontrahent</h2>\n\n                <div class=\"select\">\n                    <select name=\"client\" v-model=\"receipt.client\">\n                        <option value=\"\">- wybierz kontrahenta -</option>\n                        <option \n                            v-for=\"client in clients\"\n                            v-bind:key=\"client.id\"\n                            v-bind:value=\"client\"\n                        >\n                            {{ client.name }}\n                        </option>\n                    </select>\n                </div>\n\n                <div class=\"box mt-4\" v-if=\"receipt.client\">\n                    <p><strong>{{ receipt.client.name }}</strong></p>\n                    <p><strong>Email:</strong> {{ receipt.client.email }}</p>\n                    <p v-if=\"receipt.client.address1\">{{ receipt.client.address1 }}</p>\n                    <p v-if=\"receipt.client.address2\">{{ receipt.client.address2 }}</p>\n                    <p v-if=\"receipt.client.zipcode || receipt.client.place\">{{ receipt.client.zipcode }} {{ receipt.client.place }}</p>\n                </div>\n            </div>\n\n            <div class=\"column is-12\">\n                <h2 class=\"is-size-5 mb-4\">Produkty</h2>\n\n                <ItemForm\n                    v-for=\"item in receipt.items\"\n                    v-bind:key=\"item.item_num\"\n                    v-bind:initialItem=\"item\"\n                    @updatePrice=\"updateTotals\"\n                >\n                </ItemForm>\n\n                <button class=\"button is-light\" @click=\"addItem\">+</button>\n            </div>\n        </div>\n\n        <div class=\"column is-12\">\n            <h2 class=\"is-size-5 mb-4\">Podsumowanie</h2>\n\n            <p><strong>Do wypłaty</strong>: {{ receipt.gross_amount.toFixed(2) }} zł</p>\n        </div>\n\n        <div class=\"column is-12\">\n            <button class=\"button is-success\" @click=\"submitForm\">Zapisz</button>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\n\nimport ItemForm from '@/components/ItemForm.vue'\n\nexport default {\n    name: 'AddReceipt',\n    components: {\n        ItemForm\n    },\n    data() {\n        return {\n            receipt: {\n                receipt_number: '',\n                client: '',\n                items: [\n                    {\n                        item_num: 0,\n                        name: '',\n                        buy_price: '',\n                        quantity: 1,\n                    }\n                ],\n                gross_amount: 0\n            },\n            clients: []\n        }\n    },\n    async mounted() {\n        await this.getClients()\n    },\n    methods: {\n        getClients() {\n            axios\n                .get('/api/v1/clients/')\n                .then(response => {\n                    this.clients = response.data\n                })\n                .catch(error => {\n                    console.log(JSON.stringify(error))\n                })\n        },\n        addItem() {\n            this.receipt.items.push({\n                item_num: this.receipt.items.length,\n                category: '',\n                name: '',\n                buy_price: '',\n                quantity: 1,\n            })\n        },\n        updateTotals(changedItem) {\n            let gross_amount = 0\n\n            let item = this.receipt.items.filter(i => i.item_num === changedItem.item_num)\n\n            item = changedItem\n\n            if (item) {\n                item.name = changedItem.name\n            }\n\n            for (let i = 0; i < this.receipt.items.length; i++) {\n                gross_amount += this.receipt.items[i].gross_amount\n            }\n\n            this.receipt.gross_amount = gross_amount\n        },\n        submitForm() {\n            this.receipt.client_name = this.receipt.client.name\n            this.receipt.client_email = this.receipt.client.email\n            this.receipt.client_doc_number = this.receipt.client.doc_number\n            this.receipt.client_address1 = this.receipt.client.address1\n            this.receipt.client_address2 = this.receipt.client.address2\n            this.receipt.client_zipcode = this.receipt.client.zipcode\n            this.receipt.client_place = this.receipt.client.place\n            this.receipt.client = this.receipt.client.id\n\n            axios\n                .post('/api/v1/receipts/', this.receipt)\n                .then(response => {\n                    toast({\n                        message: 'The receipt was added',\n                        type: 'is-success',\n                        dismissible: true,\n                        pauseOnHover: true,\n                        duration: 2000,\n                        position: 'bottom-right',\n                    })\n\n                    this.$router.push('/dashboard/receipts')\n                })\n                .catch(error => {\n                    console.log(JSON.stringify(error))\n                })\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\">\n    #item-select {\n        width: 120%;\n    }\n    .select select {\n        width: 100%;\n    }\n</style>"],"mappings":";AA8EA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,KAAI,QAAS,aAAY;AAElC,OAAOC,QAAO,MAAO,2BAA0B;AAE/C,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;QACLC,cAAc,EAAE,EAAE;QAClBC,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE,CACH;UACIC,QAAQ,EAAE,CAAC;UACXP,IAAI,EAAE,EAAE;UACRQ,SAAS,EAAE,EAAE;UACbC,QAAQ,EAAE;QACd,EACH;QACDC,YAAY,EAAE;MAClB,CAAC